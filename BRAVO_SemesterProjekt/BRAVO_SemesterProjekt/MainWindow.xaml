<Window x:Class="BRAVO_SemesterProjekt.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:BRAVO_SemesterProjekt"
        mc:Ignorable="d"
        Title="MainWindow" Height="814" Width="1301">
    <Grid>
        <DockPanel Margin="0,0,0,737">
            <Menu DockPanel.Dock="Top" Height="44" Margin="0,0,0,0">
                <MenuItem Header="Opret" Height="33" MinWidth="100" FontSize="14">
                    <MenuItem Name="NewActor"  Header="Opret ny aktør" Click="NewActor_Click"  />
                    <MenuItem Name="NewProduct"  Header="Opret nyt produkt" Click="NewProduct_Click"  />
                    <MenuItem Name="NewKombi"  Header="Opret nyt kombi-produkt" Click="NewKombi_Click"  />
                    <MenuItem Name="NewCluster"  Header="Opret ny klynge" Click="NewCluster_Click"  />
                </MenuItem>
                <MenuItem Header="Oversigt" Height="33" MinWidth="100" FontSize="14">
                    <MenuItem Name="ShowActors"  Header="Hvis alle aktører" Click="ShowActors_Click"  />
                    <MenuItem Name="ShowProducts"  Header="Hvis alle produkter" Click="ShowProduct_Click"  />
                    <MenuItem Name="ShowCombos"  Header="Hvis alle kombi-produkter" Click="ShowCombos_Click"  />
                    <MenuItem Name="ShowClusters"  Header="Hvis alle klynger" Click="ShowClusters_Click"  />
                </MenuItem>
                <MenuItem Header="Deaktivere/Aktivere" Height="33" MinWidth="100" FontSize="14" Click="MenuItem_Click">
                
                </MenuItem>
                <MenuItem Header="Upload" Height="33" MinWidth="100" FontSize="14" Margin="30,0,0,0" Click="MenuItem_Click_1">
                 
                </MenuItem>
            </Menu>

        </DockPanel>
        <Frame x:Name="frame" HorizontalAlignment="Left" Height="732" VerticalAlignment="Top" Width="1293" Margin="0,51,0,0" NavigationUIVisibility="Hidden"/>


    </Grid>
</Window>
