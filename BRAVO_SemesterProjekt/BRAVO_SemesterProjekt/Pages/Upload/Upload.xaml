<Page x:Class="BRAVO_SemesterProjekt.Upload"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:BRAVO_SemesterProjekt"
      mc:Ignorable="d" 
      Background="#FFE6D431"
      d:DesignHeight="732" d:DesignWidth="1293"
      Title="Upload">

    <Grid>
        <Button x:Name="button" Content="Upload" HorizontalAlignment="Left" Margin="10,238,0,0" VerticalAlignment="Top" Width="280" Click="Upload_Click" Height="77" Background="White" FontWeight="Bold"/>
        <Button x:Name="button1" Content="Vælg Fil" HorizontalAlignment="Left" Margin="10,16,0,0" VerticalAlignment="Top" Width="123" Click="Choose_File_Click" Height="37" Background="White" FontWeight="Bold"/>
        <Label x:Name="label" Content="{Binding Path=Path}" HorizontalAlignment="Left" Margin="10,58,0,0" VerticalAlignment="Top" Height="175" Width="1273"/>
        <Label x:Name="lbl_Header" Content="Upload" HorizontalContentAlignment="Center" HorizontalAlignment="Left" Margin="133,0,0,0" VerticalAlignment="Top" Height="86" Width="1157" FontSize="20"/>

    </Grid>
</Page>
