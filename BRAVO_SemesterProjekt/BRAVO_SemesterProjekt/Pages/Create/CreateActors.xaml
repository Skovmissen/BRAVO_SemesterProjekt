<Page x:Class="BRAVO_SemesterProjekt.CreateActors"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:BRAVO_SemesterProjekt"
      mc:Ignorable="d"
      Background="#FFFCA159"
      Title="CreateActors" Height="800" Width="1300">

    <Grid>

        <TextBox x:Name="txt_Navn" Height="50" Margin="10,114,976,636" Text="{Binding Path= Name}"/>
        <TextBox x:Name="txt_Email" Height="50" Margin="10,196,976,554" Text="{Binding Path= Email }"/>
        <TextBox x:Name="txt_tlf" Height="50" Margin="10,276,976,474" Text="{Binding Path= Tlf}"/>
        <Button x:Name="button" Content="Opret aktør" Margin="10,350,890,0" VerticalAlignment="Top" Height="60" Click="btn_gem" Background="White" FontFamily="Proxima Nova Rg" FontSize="22" Foreground="#FF535353"/>
        <Button x:Name="btn_back" Content="Tilbage" Margin="10,10,1111,0" VerticalAlignment="Top" Height="60" Click="btn_back_Click" Background="White" FontFamily="Proxima Nova Rg" FontSize="22" Foreground="#FF535353"/>
        <Label x:Name="label" Content="NAVN" Margin="10,86,1111,686" FontFamily="Proxima Nova Rg" FontWeight="Bold" FontSize="14" Foreground="#FF535353"/>
        <Label x:Name="label_Copy" Content="EMAIL" Margin="10,168,1111,604" FontFamily="Proxima Nova Rg" FontWeight="Bold" FontSize="14" Foreground="#FF535353"/>
        <Label x:Name="label_Copy1" Content="TELEFON" Margin="10,252,1121,524" FontFamily="Proxima Nova Rg" FontWeight="Bold" FontSize="14" Foreground="#FF535353"/>
    </Grid>
</Page>
